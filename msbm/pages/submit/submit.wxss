
.ServiceCenter{
  color: #555555; 
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 750rpx;      /*高宽大小根据个人情况自己调试*/
    min-height: 1476rpx;
    /** 不重复*/

    background-size:150%; /**等比例缩放*/
    background-image: url('data:image/png;base64,')
}

.message{
  display: flex;
  flex-direction:column;
  align-items: center;
  border-radius: 20rpx;
  margin-top: 150rpx;
  background-color: white;
  min-height: 960rpx;
  width: 630rpx;
}

.head{
  margin-left: 15px;
  margin-right: 15px;
  display:block; 
	overflow:hidden; 
	border-radius: 50%;
	width: 156rpx;
	height: 156rpx;
  z-index: 100;
}




.detail{
  margin-top: 42rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width: 100%;

}
.input{
  text-align: right;
  width:400rpx;
}

.sex,.position{
  margin-right: 60rpx;
}

.radio{
  margin-right: 50rpx;
}


radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 26rpx; /* 对勾大小 26rpx */
  color: #fff; /* 对勾颜色 */
  background: #2facff;
  border-color: #2facff;
}


.centerPage-save-btn,button {
  width:540rpx;
  border-radius: 20rpx;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #4481dd;
  color: #fff;

  text-align: center;
}